        "lodash": "^4.17.21",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.26.10",
        "sass-loader": "^8.0.2",
        "tailwind": "^4.0.0",
        "vite": "^5.1.7",
        "vue": "^3.2.41",
        "vue-router": "^4.2.5",
        "vue-template-compiler": "^2.6.11",
